{
  "program": "p4src/router.p4",
  "language": "p4-14",
  "targets": {
    "multiswitch": {
      "model": "tofino",
      "bmv2_log": true,
      "pcap_dump": false,
      "auto-control-plane": true,
      "controller_module": "controller",
      "_cli": true,
      "links": [["h1", "s1"], ["h9", "s1"]],
      "hosts": {
        "h1": {
            "cmd": "./recv.py 1234",
            "startup_sleep": 0.1,
            "wait": false
        },
        "h9": {
            "cmd": "./send_positon.py h1:1234",
            "wait": true
        }
      },
      "switches": {
          "s1": {
              "commands": "commands.txt"
          }
      }
    }
  }
}
